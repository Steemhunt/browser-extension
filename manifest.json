{
  "name": "Steemhunt",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Daily Ranking for Effortlessly Cool Products That Rewards Hunters",
  "icons": {
    "16": "images/sh_16.png",
    "32": "images/sh_32.png",
    "48": "images/sh_48.png",
    "128": "images/sh_128.png"
  },
  "permissions": [
    "*://*.steemhunt.com/*",
    "storage",
    "tabs"
  ],
  "chrome_url_overrides": {
    "newtab": "main.html"
  },
  "web_accessible_resources": [
    "images/logo.png"
  ],
  "default_locale": "en"
}